<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Use title if it's in the page YAML frontmatter -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Work+Sans:900' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Lato:300,400' rel='stylesheet' type='text/css'>
    <%= partial 'partials/meta-seo' %>
    <%= stylesheet_link_tag :site %>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-77898904-1', 'auto');
      ga('send', 'pageview');

    </script>
  </head>

  <body class="<%= page_classes %>">
    <main>
      <!-- CONTENT SHOWS HERE -->
      <a href="http://www.altpress.com/apmasvote2016" class="fixed-btn" target="_blank">
        <h5>VOTE FOR THE APMAS HERE</h5>
      </a>
      <%= yield %>
      <div class="apmabadge">
        <a href="http://www.altpress.com/apmasvote2016" target="_blank"><img class="full-w" src="https://s3.amazonaws.com/myfangate.com/fearlessapmas2016/apmasbadge.png"></a>
      </div>
    </main>
    <%= partial 'layouts/partials/footer' %>
    <%= javascript_include_tag :all %>
  </body>
</html>
