<div class="container">
  <div class="row">
    <div class="col s12 center-align">
      <div class="header-emblem z-depth-1"><i class="fa fa-play" aria-hidden="true"></i></div>
        <div class="section-divider">
          <hr>
          <div class="chevron">
            <i class="fa fa-angle-double-down" aria-hidden="true"></i>
          </div>
        </div>
        <h3 class="no-margin apmabluetext">CHECK OUT THESE RECENT SUBMISSIONS #FEARLESSatAPMAS</h3>
      </div>
    </div>
  </div>
</div>
<div class="container">
  <!-- <div id="instafeed" class="row masonry-grid"></div> -->
  <div class="row">
    <% instagram_scraper %>
    <% @posts.each do |post| %>
    <div class="col s12 m6 l4">
      <div class="card hoverable">
        <div class="card-image"><a href="<%= post['link'] %>"><img src="<%= post['image'] %>"></a></div>
        <div class="card-content">
          <!-- <p></p> -->
        </div>
        <div class="card-action center-align"><a class="btn black" href="<%= post['link'] %>">Open Post</a></div>
      </div>
    </div>
    <% end %>
  </div>
</div>
